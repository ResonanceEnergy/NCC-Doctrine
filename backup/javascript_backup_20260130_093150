            "        window.${2:eventSource}.onmessage = function(event) {",
            "            try {",
            "                const data = JSON.parse(event.data);",
            "                handle${1:DataType}Update(data);",
            "                update${1:DataType}Display();",
            "            } catch (parseError) {",
            "                console.error(`[${1:DataType}] Parse error:`, parseError);",
            "            }",
            "        };",
            "        ",
            "        window.${2:eventSource}.onerror = function(error) {",
            "            console.error(`[${1:DataType}] Connection error:`, error);",
            "            // Implement reconnection logic",
            "            setTimeout(subscribeTo${1:DataType}Updates, 5000);",
            "        };",
            "        ",
            "        console.log(`[${1:DataType}] Real-time subscription established`);",
            "    } catch (error) {",
