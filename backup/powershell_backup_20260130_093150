{
    "NCC Function Template": {
        "prefix": "ncc-function",
        "body": [
            "# ============================================================================",
            "# ${1:FunctionName} - ${2:Description}",
            "# Generated: January 30, 2026 | Authority: AZ PRIME Command",
            "# Purpose: ${3:Purpose description}",
            "# ============================================================================",
            "",
            "function ${1:FunctionName} {",
            "    [CmdletBinding()]",
            "    param(",
            "        [Parameter(Mandatory = \\$true)]",
            "        [string]\\$AgentID,",
            "        ",
            "        [Parameter(Mandatory = \\$false)]",
            "        [switch]\\$Verbose,",
            "        ",
            "        [Parameter(Mandatory = \\$false)]",
            "        [string]\\$Division = 'NCC'",
            "    )",
            "    ",
            "    begin {",
            "        Write-Verbose \"[${1:FunctionName}] Starting execution\"",
            "        \\$startTime = Get-Date",
            "    }",
            "    ",
            "    process {",
            "        try {",
            "            # Function logic here",
            "            \\$result = @{",
            "                Status = 'Success'",
            "                AgentID = \\$AgentID",
            "                Division = \\$Division",
            "                Timestamp = Get-Date",
            "                Data = \\$null",
            "            }",
            "            ",
            "            Write-Verbose \"[${1:FunctionName}] Processing completed\"",
            "            return \\$result",
            "        }",
            "        catch {",
            "            Write-Error \"[${1:FunctionName}] Error: \\$_.Exception.Message\"",
            "            throw",
            "        }",
            "    }",
            "    ",
            "    end {",
            "        \\$duration = (Get-Date) - \\$startTime",
            "        Write-Verbose \"[${1:FunctionName}] Completed in \\$duration.TotalSeconds seconds\"",
            "    }",
            "}"
        ],
        "description": "NCC standard function template with logging and error handling"
    },

    "NCC Logging Function": {
        "prefix": "ncc-log",
        "body": [
            "Write-ImprovementLog -Message \"[${1:Component}] ${2:Message}\" -Level ${3:Information} -AgentID \\$AgentID"
        ],
        "description": "NCC standard logging call"
    },

