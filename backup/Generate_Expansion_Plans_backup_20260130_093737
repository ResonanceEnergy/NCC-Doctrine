foreach ($company in $companies) {
    $content = $template -replace "{COMPANY}", $company
    $fileName = "$company`_Expansion_Plan.md"
    $filePath = Join-Path $PSScriptRoot ".." "Doctrine" $fileName
    $content | Out-File -FilePath $filePath -Encoding UTF8
    Write-Host "Created expansion plan for $company" -ForegroundColor Green
}

Write-Host "âœ… All NCC company expansion plans generated successfully!" -ForegroundColor Green
